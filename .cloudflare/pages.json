{
  "build": {
    "publicDir": ".",
    "command": "cd nataliegwinters && npm install && npm run build",
    "cwd": ".",
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    { "pattern": "/biz/*", "script": "functions/biz.js" },
    { "pattern": "/*", "serve": "*" }
  ],
  "env": {
    "production": {
      "NODE_ENV": "production"
    },
    "preview": {
      "NODE_ENV": "development"
    }
  }
} 